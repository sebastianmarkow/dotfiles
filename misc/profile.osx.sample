# go
export GOPATH="$HOME/Developer/go"
export PATH="$GOPATH/bin:$PATH"
export PATH="/usr/local/Cellar/go/1.4/libexec/bin:$PATH"

# node
export PATH="$(brew --prefix node)/bin:$PATH"

# python
